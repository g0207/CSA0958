package rect_pattern;
import java.util.*;

class rectangle {
    public static void main(String[] Args){
        Scanner sc = new Scanner(System.in);

        int r,c;
        System.out.println("Enter Rows : ");
        r = sc.nextInt();
        System.out.println("Enter Cols : ");
        c = sc.nextInt();

        for(int i = 0;i<r;i++){
            for(int j = 0;j<c;j++){
                System.out.print("*  ");
            }
            System.out.print("\n");
        }

    }
}
